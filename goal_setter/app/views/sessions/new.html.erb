<h1>Please log in!</h1>

<form action="<%= session_url %>" method="post">
  <%= auth_token %>
  <label for="Email">Email</label>
  <input id="Email" type="text" name="session[email]" value="">
  
  <label for="Password">Password</label>
  <input id="Password" type="password" name="session[password]" value="">
  
  <input type="submit" name="" value="Sign In">
</form>
